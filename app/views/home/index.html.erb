<h1>Kakao OmniAuth Test</h1>


<% if user_signed_in? %>
  <%= link_to 'Log out', destroy_user_session_path, method: :delete, data: {confirm: 'Are you sure?'}, class: 'btn btn-danger' %>
<% else %>
  <%= link_to 'Log in', new_user_session_path, class: 'btn btn-primary' %>
  <%= link_to 'Sign up', new_user_registration_path, class: 'btn btn-primary' %>
<% end %>
